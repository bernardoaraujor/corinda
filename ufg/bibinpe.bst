            { bbl.pages pages n.dashify tie.or.space.connect }
            { bbl.page pages tie.or.space.connect }
          if$
        }{ 
          pages multi.page.check
            { type pages n.dashify tie.or.space.connect }
            { type pages tie.or.space.connect }
          if$
        }
      if$
    }
  if$
}

